<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <!-- <PatternLayout pattern="%d{yyMMdd HHmmss SSS} [%t] %-5level %logger{36} - %msg%n"/> -->
      <PatternLayout pattern="%d{yyMMdd HHmmss SSS} [%t] %-5level - %msg%n"/>
    </Console>
  </Appenders>
  
  <Loggers>
    <Root level="all">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>